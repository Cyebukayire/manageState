uter.push({ name: 'heroes' });\n    },\n    async saveHero() {\n      this.hero.id\n        ? await dataService.updateHero(this.hero)\n        : await dataService.addHero(this.hero);\n      this.$router.push({ name: 'heroes' });\n    },\n  },\n};\n",{"version":3,"sources":["hero-detail.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmDA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"hero-detail.vue","sourceRoot":"src/views","sourcesContent":["<template>\n  <div>\n    <div class=\"section content-title-group\">\n      <h2 class=\"title\">{{ title }}</h2>\n      <div class=\"card\">\n        <header class=\"card-header\">\n          <p class=\"card-header-title\">{{ hero.fullName }}</p>\n        </header>\n        <div class=\"card-content\">\n          <div class=\"content\">\n            <div class=\"field\">\n              <label class=\"label\" for=\"id\">id</label>\n              <label class=\"input\" name=\"id\" readonly>{{ hero.id }}</label>\n            </div>\n            <div class=\"field\">\n              <label class=\"label\" for=\"firstName\">first name</label>\n              <input class=\"input\" name=\"firstName\" v-model=\"hero.firstName\" />\n            </div>\n            <div class=\"field\">\n              <label class=\"label\" for=\"lastName\">last name</label>\n              <input class=\"input\" name=\"lastName\" v-model=\"hero.lastName\" />\n            </div>\n            <div class=\"field\">\n              <label class=\"label\" for=\"description\">description</label>\n              <input\n                class=\"input\"\n                name=\"description\"\n                v-model=\"hero.description\"\n              />\n            </div>\n          </div>\n        </div>\n        <footer class=\"card-footer\">\n          <button\n            class=\"link card-footer-item cancel-button\"\n            @click=\"cancelHero()\"\n          >\n            <i class=\"fas fa-undo\"></i>\n            <span>Cancel</span>\n          </button>\n          <button class=\"link card-footer-item\" @click=\"saveHero()\">\n            <i class=\"fas fa-save\"></i>\n            <span>Save</span>\n          </button>\n        </footer>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { dataService } from '../shared';\n\nexport default {\n  name: 'HeroDetail',\n  props: {\n    id: {\n      type: Number,\n      default: 0,\n    },\n  },\n  data() {\n    return {\n      hero: {},\n    };\n  },\n  async created() {\n    if (this.isAddMode) {\n      this.hero = {\n        id: undefined,\n        firstName: '',\n        lastName: '',\n        description: '',\n      };\n    } else {\n      this.hero = await dataService.getHero(this.id);\n    }\n  },\n  computed: {\n    isAddMode() {\n      return !this.id;\n    },\n    title() {\n      return `${this.isAddMode ? 'Add' : 'Edit'} Hero`;\n    },\n  },\n  methods: {\n    cancelHero() {\n      this.$router.push({ name: 'heroes' });\n    },\n    async saveHero() {\n      this.hero.id\n        ? await dataService.updateHero(this.hero)\n        : await dataService.addHero(this.hero);\n      this.$router.push({ name: 'heroes' });\n    },\n  },\n};\n</script>\n"]}]}